config interface 'loopback'
  option ifname 'lo'
  option ipaddr '127.0.0.1'
  option netmask '255.0.0.0'
  option proto 'static'

config interface 'lan'
  option ifname 'eth0'
  option proto 'static'
  option ipaddr '192.168.48.1'
  option netmask '255.255.255.0'
  option type 'bridge'

config interface 'wan'
  option proto 'dhcp'
  option hostname '703n'
