
config interface 'loopback'
  option ifname 'lo'
  option ipaddr '127.0.0.1'
  option netmask '255.0.0.0'
  option proto 'static'

config interface 'lan'
  option ifname 'eth0'
  option proto 'dhcp'
  option type 'bridge'

config interface 'wifi'
  option ipaddr '192.168.64.1'
  option netmask '255.255.255.0'
  option proto 'static'
  option type 'bridge'
